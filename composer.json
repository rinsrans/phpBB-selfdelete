{
	"name": "rinsrans/selfdelete",
	"type": "phpbb-extension",
	"description": "Allows user to delete her accounts by them self",
	"homepage": "https://github.com/rinsrans/phpBB-selfdelete",
        "keywords": ["phpbb", "extension", "user", "delete"],
	"version": "0.1.1",
	"license": "GPL-2.0",
	"authors": [{
			"name": "rinsrans",
			"email": "karl.rinser@gmail.com",
			"homepage": "http://rinsrans.ml/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "User selfdelete",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.4,<3.2.*@dev"
		}
	},
        "version-check": {
                "host": "phpbb-versionscheck.cf",
                "directory": "/rinsrans/phpBB-selfdelete/master",
                "filename": "version-check.json"
        }
}